   //! editor settings
   "files.associations": {
      "*.jsx": "javascriptreact",
      "*.tsx": "typescriptreact",
      ".sequelizerc": "javascript",
      ".stylelintrc": "json",
      ".prettierrc": "json",
      ".env.*": "dotenv"
   },

   "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],

   "tailwindCSS.includeLanguages": {
      "typescriptreact": "html"
   },

   "emmet.syntaxProfiles": {
      "javascript": "jsx"
   },
   "emmet.includeLanguages": {
      "javascript": "javascriptreact"
   },

   "files.exclude": {
      "**/.DS_Store": true,
      "**/.git": true,
      "**/.hg": true,
      "**/.svn": true,
      "**/CVS": true,
      "**/deps": true,
      "**/node_modules": true
   },

   "editor.defaultFormatter": "esbenp.prettier-vscode",
   "editor.formatOnSave": true,
   // "editor.rulers": [130],
   "editor.lineHeight": 24,
   "editor.letterSpacing": 0.4,
   // "editor.fontWeight": "10",
   // "editor.fontSize": 13.5,
   // "editor.suggest.preview": true,
   "editor.mouseWheelZoom": true,
   "editor.smoothScrolling": true,
   "editor.inlineSuggest.enabled": true,
   "editor.fontFamily": "Dank Mono, MonoLisa",
   "editor.cursorSmoothCaretAnimation": true,
   "editor.cursorWidth": 2,
   "editor.scrollbar.horizontal": "hidden",
   "editor.scrollbar.vertical": "hidden",
   "editor.fontLigatures": true,
   "editor.minimap.maxColumn": 50,
   "editor.minimap.renderCharacters": false,
   "editor.wordWrap": "on",
   "workbench.editor.highlightModifiedTabs": true,
   "workbench.startupEditor": "none",
   "workbench.sideBar.location": "right",
   "workbench.colorTheme": "One Dark Pro",
   "editor.guides.indentation": true,
   "editor.guides.highlightActiveIndentation": true,

   "files.trimTrailingWhitespace": true,
   "files.trimFinalNewlines": true,
   "files.insertFinalNewline": true,

   "liveServer.settings.donotShowInfoMsg": true,
   "liveServer.settings.donotVerifyTags": true,

   "breadcrumbs.enabled": true,

   "html.format.wrapLineLength": 90,

   "tabnine.experimentalAutoImports": true,
   "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",

   "tailwindCSS.emmetCompletions": true,

   "css.lint.duplicateProperties": "warning",

   "typescript.suggest.paths": true,
   "path-intellisense.extensionOnImport": true,

   "terminal.integrated.fontSize": 19,
   "terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",

   "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font, MonoLisa",

   "javascript.updateImportsOnFileMove.enabled": "always",
   "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
   "javascript.suggest.autoImports": true,

   "typescript.suggest.autoImports": true,
   "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,

   "explorer.confirmDragAndDrop": false,

   "security.workspace.trust.untrustedFiles": "open",

   "prettier.jsxBracketSameLine": true,
   "prettier.jsxSingleQuote": true,
   "prettier.singleQuote": true,
   "prettier.tabWidth": 3,
   "prettier.useTabs": false,
   "prettier.semi": true,
   "prettier.printWidth": 120,

   // Decoration color for small packages
   "importCost.smallPackageColor": "#66cc8d",
   // Decoration color for medium packages
   "importCost.mediumPackageColor": "#c6de68",
   // Decoration color for large packages
   "importCost.largePackageColor": "#d44e40",

   "editor.suggestSelection": "first",

   "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",

   "typescript.updateImportsOnFileMove.enabled": "always",

   "editor.linkedEditing": true,

   "workbench.productIconTheme": "material-product-icons",

   "workbench.iconTheme": "vscode-icons",
   "editor.cursorBlinking": "expand",
   "workbench.editor.enablePreview": false,
   "editor.autoClosingBrackets": "always",
   "editor.autoClosingDelete": "always",
   "editor.autoClosingQuotes": "always",
   "editor.fastScrollSensitivity": 7,
   "editor.inlayHints.fontFamily": "JetBrains Mono",
   "editor.mouseWheelScrollSensitivity": 1.5,
   "editor.tabCompletion": "on",
   "terminal.integrated.fontWeight": "bold",
   "editor.renderWhitespace": "boundary",
   "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
   },

   "editor.quickSuggestionsDelay": 1,
   "window.menuBarVisibility": "compact",
   "terminal.integrated.cursorStyle": "underline",
   "terminal.integrated.letterSpacing": -1,
   "terminal.integrated.minimumContrastRatio": 4.5,
   "oneDarkPro.italic": false,

   "editor.codeActionsOnSave": {
      "source.organizeImports": true
   }

