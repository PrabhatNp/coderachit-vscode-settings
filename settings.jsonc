  //! token settings
  "editor.tokenColorCustomizations": {
    "comments": "#616380",
    "textMateRules": [
      {
        "scope": [
          "entity.name.function",
          "entity.name.method",
          "entity.name.tag.wildcard.css",
          "entity.name.tag.css"
          // "support.type.property-name.json.comments"
          // "meta.brace.square.tsx"
          // "entity.name.tag"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      },

      {
        "scope": ["support.class.component.js", "support.class.component.tsx"],
        "settings": {
          "fontStyle": "underline"
        }
      },

      {
        "scope": [
          //!for operator mono / iScript uncomment below if commented
          "keyword.control.loop",
          "keyword.control.import",
          "keyword.control.export",
          "keyword.control.from",
          "keyword.control.flow",
          "keyword.control.conditional",
          "keyword.control.switch",
          "keyword.control.default",
          "keyword.operator.new",
          "entity.other.attribute-name.id.css",
          "entity.other.attribute-name",
          "storage.type.js",
          "storage.type.function.js",
          "storage.type.function.tsx",
          "storage.type.tsx",
          "variable.language.this",
          "keyword.control.trycatch",
          "comment"
          // "string.quoted.double"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },

      {
        "scope": ["variable.language.this"],
        "settings": {
          "foreground": "#ff9102"
        }
      },

      {
        "scope": [
          "keyword.control.import",
          "keyword.control.export",
          "keyword.operator.new"
        ],
        "settings": {
          "foreground": "#a57ee7"
        }
      },

      {
        "scope": ["support.class.component"],
        "settings": {
          "foreground": "#dfe65ad5"
        }
      },

      {
        "scope": ["storage.type"],
        "settings": {
          "foreground": "#f764d7"
        }
      },

      {
        "scope": ["variable.other.readwrite.alias"],
        "settings": {
          "foreground": "#ff6666e1"
        }
      },

      {
        "scope": ["constant.language"],
        "settings": {
          "foreground": "#fde845bd"
        }
      },

      {
        "scope": ["support.constant.property-value.css"],
        "settings": {
          "foreground": "#ff4aae",
          "fontStyle": "italic"
        }
      },

      {
        "scope": ["constant.numeric"],
        "settings": {
          "foreground": "#b56df8"
        }
      },

      {
        "scope": ["keyword.other.unit"],
        "settings": {
          "foreground": "#68f774fb"
        }
      },

      {
        "scope": ["string.quoted.double"],
        "settings": {
          "foreground": "#6fee73de"
        }
      },

      {
        "scope": ["entity.other.attribute-name.class.css"],
        "settings": {
          "foreground": "#f19b2a"
        }
      },

      {
        "scope": ["variable.other"],
        "settings": {
          "foreground": "#f56565"
        }
      },

      {
        "scope": [
          "entity.name.tag.css"
          // "entity.name.tag",
        ],
        "settings": {
          "foreground": "#e04848"
        }
      },

      {
        "scope": ["text.html.derivative"],
        "settings": {
          "foreground": "#acacac"
        }
      },

      {
        "scope": ["entity.other.attribute-name"],
        "settings": {
          "foreground": "#fcae5c"
        }
      }
    ]
  },

  //! editor settings
  "files.associations": {
    "*.jsx": "javascriptreact"
  },

  // "eslint.validate": [
  //   "javascript",
  //   "javascriptreact",
  //   "typescript",
  //   "typescriptreact"
  // ],

  "tailwindCSS.includeLanguages": {
    "typescriptreact": "html"
  },

  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },

  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/deps": true,
    "**/node_modules": true
  },

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.lineHeight": 20,
  "editor.letterSpacing": 0.25,
  "editor.fontWeight": "90",
  "editor.mouseWheelZoom": true,
  "editor.smoothScrolling": true,
  "editor.inlineSuggest.enabled": true,
  "editor.fontFamily": "Fira Code iScript Light, Cascadia Code, Anonymous Pro, Operator Mono",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 2,
  "editor.renderWhitespace": "boundary",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.fontLigatures": true,
  "editor.minimap.maxColumn": 80,
  "editor.minimap.renderCharacters": false,
  "editor.wordWrap": "on",
  "editor.fontSize": 13.5,
  "editor.bracketPairColorization.enabled": true,
  "editor.suggest.preview": true,

  "workbench.startupEditor": "none",
  "workbench.sideBar.location": "right",
  "workbench.colorTheme": "One Dark Pro Flat",
  "workbench.iconTheme": "material-icon-theme",

  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,

  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,

  "breadcrumbs.enabled": true,

  "html.format.wrapLineLength": 150,

  "tabnine.experimentalAutoImports": true,

  "window.zoomLevel": -1,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",

  "tailwindCSS.emmetCompletions": true,

  "css.lint.duplicateProperties": "warning",

  "typescript.suggest.paths": true,
  "path-intellisense.extensionOnImport": true,

  "terminal.integrated.fontSize": 23,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontFamily": "Cascadia Code",

  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,

  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,

  "explorer.confirmDragAndDrop": false,

  "security.workspace.trust.untrustedFiles": "open",

  "prettier.jsxBracketSameLine": true
}
